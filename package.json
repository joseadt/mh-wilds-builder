{
    "name": "mh-wilds-builder",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "generate-schema": "npm run generate-schema:armor && npm run generate-schema:skill && npm run generate-schema:decoration",
        "generate-schema:armor": "typescript-json-schema src/app/models/armor.model.ts Armor --out public/schemas/armor-schema.json ",
        "generate-schema:skill": "typescript-json-schema src/app/models/skill.model.ts Skill --out public/schemas/skill-schema.json",
        "generate-schema:decoration": "typescript-json-schema src/app/models/decoration.model.ts Decoration --out public/schemas/decoration-schema.json"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^19.1.0",
        "@angular/cdk": "^19.1.4",
        "@angular/common": "^19.1.0",
        "@angular/compiler": "^19.1.0",
        "@angular/core": "^19.1.0",
        "@angular/forms": "^19.1.0",
        "@angular/platform-browser": "^19.1.0",
        "@angular/platform-browser-dynamic": "^19.1.0",
        "@angular/router": "^19.1.0",
        "mh-wilds-builder": "file:",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.15.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^19.1.7",
        "@angular/cli": "^19.1.7",
        "@angular/compiler-cli": "^19.1.0",
        "@angular/localize": "^19.1.6",
        "@types/jasmine": "~5.1.0",
        "jasmine-core": "~5.5.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "typescript": "~5.7.2",
        "typescript-json-schema": "^0.65.1"
    }
}
